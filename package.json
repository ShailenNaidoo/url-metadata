    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-rate-limit": "^3.4.0",
    "graphql": "^14.1.1",
    "graphql-custom-types": "^1.5.0",
    "himalaya": "^1.1.0",
    "jsdom": "^15.1.0",
    "lodash": "^4.17.11",
    "node-cache": "^4.2.0",
    "url-assembler": "^2.0.0",
    "url-parse": "^1.4.4"
  },
